<thead class="text-c">
  <tr>
    <th>客户名称</th>
    <th>客户类型</th>
    <th>联系人</th>
    <th>联系电话</th>
    <th>地址</th>
    <th>状态</th>
    <th>设备数量</th>
    <th>操作</th>
  </tr>
</thead>
<tbody class="text-c">
{{#if this}}
{{#each this}}
    <tr>
        <td>{{companyName}}</td>
        <td>{{typeName}}</td>
        <td>{{linkMan}}</td>
        <td>{{phone}}</td>
        <td>{{province}}{{city}}{{address}}</td>
        {{#ifCond status "==" 0}}<td class="c-red">禁用</td>{{else}}<td class="c-green">启用</td>{{/ifCond}}
        <td>{{deviceCount}}</td>
        <td><a href="/company/customerDetail.htm?companyId={{companyId}}" class="detailInfor">详情</a></td>
    </tr>
{{/each}}
{{else}}
    <tr>
        <td class="text-c" colspan="8">暂无数据</td>
    </tr>
{{/if}} 
</tbody>